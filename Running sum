Input - 1,2,3,4,5;
output- 1,3,6,10,15;



1,1+2,2+3,3+4.....;
Logic very simple


class Solution {
    public int[] runningSum(int[] nums) {
        int result[];
        result[0]=nums[0];
        for(int i=1;i<nums.length;i++){
            result[i]=result[i-1]+ nums[i];
        }
        return result[i];
        
        
    }
}
